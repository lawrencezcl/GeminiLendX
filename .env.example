# Environment variables for GeminiLend X

# Server configuration
PORT=3000

# ZetaChain configuration
ZETA_CHAIN_API_URL=https://api.zetachain.com
ZETA_CHAIN_API_KEY=your_zeta_chain_api_key_here

# Google Gemini configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Database configuration
DATABASE_URL=postgresql://user:password@localhost:5432/gemini_lend_x
REDIS_URL=redis://localhost:6379

# Chainlink configuration
CHAINLINK_API_URL=https://api.chain.link

# CoinGecko configuration
COINGECKO_API_KEY=your_coingecko_api_key_here

# SendGrid configuration
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Wallet configuration
METAMASK_SDK_VERSION=0.10.0
PHANTOM_SDK_VERSION=2.5.0
ZETA_WALLET_ADAPTER_VERSION=2.0